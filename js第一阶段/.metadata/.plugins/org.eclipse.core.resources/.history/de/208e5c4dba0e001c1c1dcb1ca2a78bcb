<body>
    <div>1</div>
    <div>2</div>
    <div>3</div>
    
    <script type="text/javascript" src="jquery.js"></script>
    <script type="text/javascript">
        $(function () {
        	//index 下标    dom： js的dom元素
			$('div').each(function (index, dom) {
				$(dom).css('background', 'pink');
			})
		})
    </script>
</body>
</html>